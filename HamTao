//Hàm tạo mặc định
public PhanSo()
{

}

//Hàm tạo không truyền tham số
public PhanSo()
{
  this.TuSo = 0;
  this.MauSo = 1;
}

//Hàm tạo có truyền tham số
public PhanSo(int t, int m)
{
  this.TuSo = t;
  if(m != 0)
  {
    this.MauSo = m;
  }
  else
  {
    this.MauSo = 1;
  }
}

//Hàm tạo sao chép
public PhanSo(PhanSo A)
{
  this.TuSo = A.TuSo;
  this.MauSo = A.MauSo;
}
